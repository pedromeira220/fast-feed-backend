{
  "name": "fast-feed-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "lint": "npx eslint src --ext .ts",
    "lint:fix": "npx eslint src --ext .ts --fix",
    "prisma:migrate:dev": "DB_HOST=\"localhost\" npx prisma migrate dev",
    "prisma:migrate:deploy": "DB_HOST=\"localhost\" npx prisma migrate deploy",
    "prisma:studio": "DB_HOST=\"localhost\" npx prisma studio",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "test:e2e:watch": "vitest --config ./vitest.config.e2e.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pedromeira220/fast-feed-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pedromeira220/fast-feed-backend/issues"
  },
  "homepage": "https://github.com/pedromeira220/fast-feed-backend#readme",
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@rocketseat/eslint-config": "^2.1.0",
    "@swc/core": "^1.3.78",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.8.7",
    "@types/supertest": "^2.0.12",
    "@vitest/coverage-v8": "^0.34.2",
    "dotenv": "^16.3.1",
    "prisma": "^5.5.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.0",
    "unplugin-swc": "^1.3.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.5",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@prisma/client": "^5.5.2",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "eslint": "^8.52.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "zod": "^3.22.4",
    "zod-validation-error": "^2.1.0"
  }
